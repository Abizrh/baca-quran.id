{
  "compilerOptions": {
    "target": "es5",
    "module": "esnext",
    "moduleResolution": "node",
    "lib": [
      "esnext",
      "esnext.asynciterable",
      "dom"
    ],
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "resolveJsonModule": true,
    "allowJs": true,
    "sourceMap": true,
    "strict": true,
    "noImplicitAny": false,
    "noEmit": true,
    "baseUrl": ".",
    "paths": {
      "~/*": [
        "./*"
      ],
      "@/*": [
        "./*"
      ]
    },
    "types": [
      "@types/jest",
      "@types/node",
      "@nuxt/vue-app"
    ]
  },
  "exclude": [
    "node_modules/**",
    "dist/**",
    "dist.js/**",
    ".nuxt/**",
    "nuxt.config.ts",
    "tests/**"
  ]
}
